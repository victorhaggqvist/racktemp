{% extends '::rtbase.html.twig' %}

{% block container %}
  <div class="row">
    <div class="col-md-6">
      <h3>Today</h3>
      <div id="today"></div>
    </div>
    <div class="col-md-6">
      <h3>Week</h3>
      <div id="week"></div>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <h3>Month</h3>
      <div id="month"></div>
    </div>
  </div>
{% endblock %}

{% block pagescript %}
<script>
  RackTemp.setApiInfo({{ webKey|raw }}); // jshint ignore:line
  RackTemp.createChartToday();
  RackTemp.createChartWeek();
  RackTemp.createChartMonth();
</script>
{% endblock %}
